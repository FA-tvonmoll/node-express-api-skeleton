{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "functions": "never",
        "objects": "always-multiline"
      }
    ],
    "strict": ["error", "global"]
  }
}
